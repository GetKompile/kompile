.SH "NAME"
.sp
pom\-generate \-
.SH "SYNOPSIS"
.sp
\fBpom\-generate\fP [\fB\-\-cli\fP] [\fB\-\-debug\fP] [\fB\-\-dl4j\fP] [\fB\-\-image\fP] [\fB\-\-nd4j\fP] [\fB\-\-nd4j\-tensorflow\fP]
